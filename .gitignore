# ignore all files starting with .
.*

# track files starting  with .
!.gitkeep
!.editorconfig
!.eslintrc.js

# track this file .gitignore (i.e. do NOT ignore it)
!.gitignore

# track readme.md in the root (i.e. do NOT ignore it)
!readme.md

# track .htaccess
!.htaccess

# ignore all files that start with ~
~*

# ignore OS generated files
ehthumbs.db
Thumbs.db

# ignore Editor files
*.sublime-project
*.sublime-workspace
*.komodoproject

# ignore log files and databases
*.log
*.sql
*.sqlite

# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip


# ignore unwanted wordpress directories and files
core/
content/*
content/cache/
!content/themes/
!content/mu-plugins
content/uploads/cache
content/uploads/mailpoet/
!content/uploads/
!import/*.sql
vendor/
wp-config.php